# DESAFIO 004
sig = input('Escreva qualquer coisa: ')
print(sig.isalnum()) # Alfanúmerico
print(sig.isalpha()) # Alfabeto
print(sig.islower()) # Maiúsculas
print(sig.isspace()) # Espaço
print(sig.isnumeric()) # Número
print(sig.isupper()) # Minúsculas
print(sig.istitle()) # Capitalizada
